{
  "expo": {
    "name": "7 Tunes",
    "slug": "7-tunes",
    "version": "6.8.6",
        "scheme": "prod",

    "newArchEnabled": true,
    "orientation": "default",
    "icon": "./assets/images//logofinal.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logofinal.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5DE"
    },
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "2021c9d7-dd0b-4c29-8a9d-da78153a1d49"
      }
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-updates",
        {
          "username": "carolous.ghobrial"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-secure-store",
      "expo-router"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/2021c9d7-dd0b-4c29-8a9d-da78153a1d49"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "UIStatusBarHidden": true,
        "NSInAppPurchaseUsageDescription": "7Tunes requires access to in-app purchases to make purchases."
      },
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.companyname.TasbehaFinal"
    },
    "android": {
      "permissions": [
        "com.android.vending.BILLING"
      ],
      "versionCode": 6222250,
      "adaptiveIcon": {
        "backgroundColor": "#F5F5DE",
        "foregroundImage": "./assets/images/logofinal.png"
      },
      "package": "com.seventunes.tasbehafinal"
    },
    "web": {
      "favicon": "./assets/images/logofinal.png"
    },
    "runtimeVersion": "50.0.0"
  }
}
