{
  "expo": {
    "name": "7Tunes",
    "slug": "7Tunes",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images//logofinal.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logofinal.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5DE"
    },
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "5d77844a-fb3a-45d6-8695-bc7427964624"
      }
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "UIStatusBarHidden": true,
        "NSInAppPurchaseUsageDescription": "YOUR APP NAME requires access to in-app purchases to make purchases."
      },
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.companyname.TasbehaFinal"
    },
    "android": {
      "permissions": [
        "com.android.vending.BILLING"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.companyname.TasbehaFinal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
